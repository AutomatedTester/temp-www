<!DOCTYPE html>
<html data-api-page="/api/<%- subPageName %>">
<head>
  <%- content['_includes_']['header'] %>
  <style>
    [data-api-page="/api/<%- subPageName %>"] .btn-toggle-nav li>a[href$="/api/<%- subPageName %>"]{
      background: #f2f2f2;
      color: #444;
    }

    [data-api-page="/api/<%- subPageName %>"] [data-page-uri="/api/<%- subPageName %>"]{
      display: block;
    }
  </style>
</head>
<body>
  <!-- Google Tag Manager (noscript) -->
  <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-MWC4FHS"
  height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
  <!-- End Google Tag Manager (noscript) -->
<%- content['_includes_']['navbar'] %>
<%- content['_includes_']['parallax'] %>

<section id="apimethod-container" data-page-uri="/api/$method" class="secondary">
  <div class="background">
    <div class="container bs-docs-container">
      <div class="row">
        <div class="col-md-3 bs-sidebar hidden-print" role="complementary">
          <%- content['_includes_']['api-sidebar'] %>
        </div>

        <div class="col-md-9" role="main">
          <div class="docs-section" data-page-uri="/api/<%- subPageName %>">
            <div class="page-content">
              <%- pageContent %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

<section id="index-container" data-page-uri="/"></section>
<section id="guide-container" data-page-uri="/guide" class="secondary"></section>
<section id="api-container" data-page-uri="/api/index" class="secondary"></section>
<section id="about-container" data-page-uri="/about" class="secondary"></section>
<section id="releases-container" data-page-uri="/releases" class="secondary"></section>

<%- content['_includes_']['footer'] %>
<%- content['_includes_']['scripts'] %>
<script>
  (function() {
    setTimeout(function() {
      try {
        $('#navbartop ul').on('click', 'li a', function(ev) {
          ev.stopPropagation();
        });

        var listElement = $('#commands-collapse .btn-toggle-nav li>a[href$="/api/<%- subPageName %>"]');

        if (listElement && listElement.get(0)) {
          var scrollTop = listElement.get(0).offsetTop;
          var sideBar = $('#apimethod-container .bs-sidebar');

          if (scrollTop > window.visualViewport.height) {
            sideBar.addClass('scrollarea');
            sideBar[0].style.maxHeight = '90vh';
            sideBar[0].scrollTop = scrollTop - 200;
          }

        }
      } catch (err) {
      }

    }, 50);
  })();

</script>
</body>
</html>
