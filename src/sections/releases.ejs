<div class="jumbotron">
  <div class="container" style="height:130px">
    <div class="row">
      <div class="col-md-8">
        <h1>Releases</h1>
      </div>
      <div class="col-md-4">
        <div class="container-carbon"></div>
      </div>
    </div>
  </div>
</div>

<div class="background">
  <div class="container bs-docs-container">
    <div class="row">
      <div class="col-md-2"></div>

      <div class="col-md-10" role="main">
        <div class="docs-section">
          <div class="page-header">
            <h2>Version Changelog</h2>
          </div>
          <div class="list-group">
            <% content['releases'].forEach(function(release) { %>
            <div class="list-group-item">
              <div class="d-flex w-100 justify-content-between">
                <h3 class="mb-1"><a href="<%- release['html_url'] %>"><%- release['name'] %></a></h3>
                <em><%- release['timeAgo'] %></em>
              </div>
              <p class="mb-1"><%- release['body'] %></p>
            </div>
            <% }); %>
          </div>
          <h5 class="list-group-item list-group-item-action all-releases"><a href="https://github.com/nightwatchjs/nightwatch/releases" target="_blank">All releases</a></h5>
        </div>
      </div>
    </div>
  </div>
</div>
