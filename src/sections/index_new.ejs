<script>
  function copytoclipboard(element) {
    var $temp = $("<input>");
    $("body").append($temp);
    $(element).hide();
    $temp.val('npm init nightwatch').select();
    document.execCommand("copy");
    $temp.remove();
    $('.action').css('background', '#0F720B');
    $('.npm-text').addClass('regular16px').removeClass('npm-text').text('Code copied').css('color', '#FFFFFF');
    $('.check-circle').css('display', 'block');
    setTimeout(function () {
      $('.action').css('background', '#FFFFFF');
      $(element).show();
      $('.check-circle').css('display', 'none');
      $('.npm-install .regular16px').removeClass('regular16px').text('$ npm init nightwatch').addClass('npm-text').css('color', '#787878');
    }, 5000);
  }

  $(document).ready(function () {

