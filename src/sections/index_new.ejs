          variableWidth: true,
          dots: true,
          infinite: true,
          mobileFirst: true,
          arrows : false,
          autoplay : true,
          autoplaySpeed : 3000,
          swipeToSlide : true,
          responsive: [
            {
              breakpoint: 1008,
              settings: "unslick"
            }
          ]
        });
      }
    }

    $(window).resize(function () {
      enableSlideSlicker ();
    });

    if ($(window).width() > 678 && $(window).width() < 1007) {
      enableSlideSlicker ();
    }
  });
</script>